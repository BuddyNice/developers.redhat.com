{#
/**
 * @file assembly.html.twig
 * Default theme implementation to present Assembly data.
 *
 * This template is used when viewing Assembly pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_assembly()
 *
 * @ingroup themeable
 */
#}

<div{{ attributes.addClass('assembly pf-l-grid') }}{{ audience_selection }}>
  <div class="pf-l-grid__item pf-m-10-col">
      {% if content %}
      {{ content.field_title }}
      <hr class="rhd-c-divider pf-u-mt-lg">
      {{ content.field_content_list }}
      <hr class="rhd-c-divider pf-u-mb-lg">
      {% endif %}
  </div>
</div>
